<script>
      var toggleMenu = function(el) {
        var that = $('#everything');
        if(that.hasClass('menu-open')) {
          that.removeClass('menu-open');
        } else {
          that.addClass('menu-open');
        }
      }     
      $(window).resize(function() {
        if($('#everything').hasClass('menu-open') && !$('.app-navigation-toggle').is(':visible')) {
          $('#everything').removeClass('menu-open');
        }
      });
</script>
    

